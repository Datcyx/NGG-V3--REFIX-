CMD:togsnow(playerid, params[])
{
    if(value[playerid] == false)
    {
        new Float:Position[3];
        GetPlayerCameraPos(playerid, Position[0], Position[1], Position[2]);
        FallingSnowObject[playerid] = CreatePlayerObject(playerid, 18864, Position[0], Position[1], Position[2]-5, 0.0, 0.0, 0.0, 300.0 );
        SendClientMessage(playerid, -1, "You have enabled falling snow for your client!");
        value[playerid] = true;
    }
    else if(value[playerid] == true)
    {
        DestroyPlayerObject(playerid, FallingSnowObject[playerid]);
        SendClientMessage(playerid, -1, "You have disabled falling snow for your client!");
        value[playerid] = false;
    }
    return true;
}
